    "test:bdd": "vitest run tests/bdd",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "demo": "node demo.mjs"
  },
  "files": [
    "index.js",
    "cleanroom-runner.js",
    "local-runner.js",
    "assertions.js",
    "scenario-dsl.js",
