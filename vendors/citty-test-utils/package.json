{
  "name": "citty-test-utils",
  "version": "0.2.3",
  "description": "Testing utilities for GitVan CLI with Docker cleanroom support and fluent assertions",
  "type": "module",
  "main": "index.js",
  "types": "types.d.ts",
  "keywords": [
    "testing",
    "cli",
    "docker",
    "testcontainers",
    "gitvan",
    "citty",
    "cleanroom",
    "assertions"
  ],
  "author": "GitVan Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seanchatmangpt/gitvan.git",
    "directory": "vendors/citty-test-utils"
  },
  "bugs": {
    "url": "https://github.com/seanchatmangpt/gitvan/issues"
  },
  "homepage": "https://github.com/seanchatmangpt/gitvan#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "testcontainers": "^10.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.1",
    "vitest": "^1.0.0"
  },
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
